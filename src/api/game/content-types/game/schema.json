{
  "kind": "collectionType",
  "collectionName": "games",
  "info": {
    "singularName": "game",
    "pluralName": "games",
    "displayName": "Game",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "minPlayers": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "default": 2,
      "min": 0
    },
    "maxPlayers": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer"
    },
    "playingTime": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "min": 1
    },
    "minPlaytime": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "min": 1
    },
    "maxPlaytime": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer"
    },
    "minAge": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "min": 1
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::gen-qui.gen-qui",
      "inversedBy": "games_created"
    },
    "types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::type.type",
      "inversedBy": "games"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::categorie.categorie",
      "inversedBy": "games"
    },
    "mecanismes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::mecanisme.mecanisme",
      "inversedBy": "games"
    },
    "complexity": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "decimal",
      "max": 10,
      "min": 1
    },
    "extension_de": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::game.game",
      "inversedBy": "Extensions"
    },
    "Extensions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::game.game",
      "mappedBy": "extension_de"
    },
    "Titre": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": true
    },
    "illustrateurs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::gen-qui.gen-qui",
      "inversedBy": "games_illustrated"
    },
    "mainVersion": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": true
    },
    "language": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "Français",
        "Anglais",
        "Allemand",
        "Espagnol",
        "Russe",
        "Hollandais"
      ],
      "default": "Français"
    },
    "version": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "editors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::editeur.editeur",
      "inversedBy": "games"
    },
    "img_box": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "yearPublished": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "caption": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "maxLength": 240
    },
    "forum_links": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "component": "liens.forum"
    },
    "Version": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "version.version"
      ]
    },
    "Casting": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "internes.gens"
      ]
    }
  }
}
